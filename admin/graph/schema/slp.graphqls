type SlpGenesis {
    tx: Tx!
    hash: String!
    decimals: Uint8!
    output: SlpOutput!
    baton: SlpBaton!
    ticker: String!
    name: String!
    doc_url: String!
    doc_hash: String!
}

type SlpOutput {
    output: TxOutput!
    hash: String!
    index: Uint32!
    amount: Int64!
    genesis: SlpGenesis
}

type SlpBaton {
    output: TxOutput!
    hash: String!
    index: Uint32!
    genesis: SlpGenesis
}

